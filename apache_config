Alias /static/admin/ /usr/local/lib/python2.7/dist-packages/django/contrib/admin/static/admin/

    WSGIDaemonProcess site-1 user=www-data group=www-data threads=25
    WSGIProcessGroup site-1

    Alias /hbmedia/ /srv/www/hbmedia/

    <Directory /srv/www/hbmedia>
        Order deny,allow
        Allow from all
    </Directory>

    WSGIScriptAlias /hb /srv/www/hb/apache/django.wsgi

    <Directory /srv/www/hb/apache>
        Order deny,allow
        Allow from all
        XSendFile On
        XSendFilePath "/tmp"
        XSendFilePath "/"
    </Directory>
